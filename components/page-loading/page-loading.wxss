/* components/page-loading/page-loading.wxss */
.page-loading{
  position: fixed;
  top: 65px;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #F4F4F5;
}

.page-loading-part{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: #F4F4F5;
  z-index: 100;
}

.loading-img{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}